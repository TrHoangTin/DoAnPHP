{
    "name": "tranhoangtin/webbanhang",
    "description": "Website bán hàng với PHP",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "firebase/php-jwt": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}